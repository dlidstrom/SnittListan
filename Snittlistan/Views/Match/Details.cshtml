@model MatchViewModel
@{
    ViewBag.Title = "Detaljer";
}

<h2>@ViewBag.Title</h2>
@Html.GenerateEditBackLinks(Model.Match)

<fieldset>
    <legend>Match</legend>
    @Html.DisplayFor(model => model.Match, "Details")
    <div class="games-viewmodel">
        <h3>Hemmalag</h3>
        @Html.DisplayFor(model => model.HomeTeam, "Team")
    </div>
    <div class="games-viewmodel">
        <h3>Bortalag</h3>
        @Html.DisplayFor(model => model.AwayTeam, "Team")
    </div>
</fieldset>