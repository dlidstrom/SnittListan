@model Snittlistan.Web.Models.SendEmail
To: @Model.To
From: @Model.From
@if (string.IsNullOrEmpty(Model.ReplyTo) == false)
{
    @:Reply-To: @Model.ReplyTo
}
Bcc: @Model.Bcc
Subject: @Model.Subject
Content-Type: text/html; charset=utf-8

<html>
<head>
</head>
<body>
    @Model.Content
</body>
</html>
