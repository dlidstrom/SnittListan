@model RegisterResult

<div class="control-group">
    <label class="control-label" for="rosterid">Match:</label>
    <div class="controls">
        @Html.DropDownList("rosterid", "Välj match")
    </div>
</div>
<div class="control-group">
    <label class="control-label" for="teamScore">Lagpoäng:</label>
    <div class="controls">
        <input id="teamScore"
               name="teamScore"
               type="text"
               pattern="\d*"
               min="0"
               max="20"
               value="@Model.TeamScore"
               required="required" />
    </div>
</div>
<div class="control-group">
    <label class="control-label" for="opponentScore">Motståndarpoäng:</label>
    <div class="controls">
        <input id="opponentScore"
               name="opponentScore"
               type="text"
               pattern="\d*"
               min="0"
               max="20"
               value="@Model.OpponentScore"
               required="required" />
    </div>
</div>
<div class="control-group">
    <label class="control-label" for="bitsMatchId">BITS Match Id:</label>
    <div class="controls">
        <input id="bitsMatchId"
               name="bitsMatchId"
               type="text"
               pattern="\d*"
               value="@Model.BitsMatchId"
               required="required" />
    </div>
</div>
